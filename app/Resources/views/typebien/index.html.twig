{% extends 'base.html.twig' %}

{% block body %}
    <h1>Typebiens list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for typeBien in typeBiens %}
            <tr>
                <td><a href="{{ path('typebien_show', { 'id': typeBien.id }) }}">{{ typeBien.id }}</a></td>
                <td>{{ typeBien.nom }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('typebien_show', { 'id': typeBien.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('typebien_edit', { 'id': typeBien.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('typebien_new') }}">Create a new typeBien</a>
        </li>
    </ul>
{% endblock %}
